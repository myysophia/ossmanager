# ===== 应用配置 =====
APP_PORT=8080

# ===== Docker 镜像配置 =====
# 阿里云容器镜像仓库地址（如果使用预构建镜像）
ACR_REGISTRY=your-acr-registry-url

# ===== JWT 配置 =====
# 请修改为您的安全密钥，至少32字符
JWT_SECRET_KEY=your-super-secret-jwt-key-change-me-in-production-32chars

# ===== 数据库配置 =====
# 使用内置 PostgreSQL 数据库（推荐）
POSTGRES_DB=ossmanager
POSTGRES_USER=ossuser
POSTGRES_PASSWORD=your-secure-database-password
POSTGRES_PORT=5432
DB_SSLMODE=disable

# 使用外部数据库时取消注释以下配置，并注释上面的内置数据库配置
# DB_HOST=your-external-database-host
# DB_PORT=5432
# DB_USERNAME=your-database-username
# DB_PASSWORD=your-database-password
# DB_NAME=your-database-name
# DB_SSLMODE=require

# ===== OSS 存储配置（可选，不使用OSS时可以留空） =====

# 阿里云 OSS 配置
ALIYUN_ACCESS_KEY_ID=
ALIYUN_ACCESS_KEY_SECRET=
ALIYUN_OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com
ALIYUN_OSS_BUCKET=
ALIYUN_OSS_REGION=cn-hangzhou
ALIYUN_OSS_UPLOAD_DIR=uploads/

# 阿里云函数计算（可选）
ALIYUN_FC_ENDPOINT=
ALIYUN_FC_ACCESS_KEY_ID=
ALIYUN_FC_ACCESS_KEY_SECRET=
ALIYUN_FC_SERVICE_NAME=oss-manager
ALIYUN_FC_FUNCTION_NAME=calculate-md5

# AWS S3 配置
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=
AWS_S3_UPLOAD_DIR=uploads/

# Cloudflare R2 配置
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_R2_ACCESS_KEY_ID=
CLOUDFLARE_R2_SECRET_ACCESS_KEY=
CLOUDFLARE_R2_BUCKET=
CLOUDFLARE_R2_UPLOAD_DIR=uploads/
